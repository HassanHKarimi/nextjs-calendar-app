{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install --no-audit --no-fund",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "env": {
    "NEXT_TYPESCRIPT_COMPILE_ONLY": "1",
    "NEXT_TELEMETRY_DISABLED": "1",
    "NEXT_SKIP_COMPILE_EVENT": "true",
    "NEXT_FORCE_EDGE_IMAGES": "true"
  }
}